{
	"name": "wikibase/mediawiki-term-store",
	"license": "BSD-3-Clause",
	"description": "Small library for persistence of Wikibase terms via MediaWikis database abstraction layer",
	"require": {
		"php": "^5.6.99|^7.0",
		"wikibase/term-store": "~1.0",
		"wikibase/data-model": "^9.1"
	},
	"require-dev": {
		"phpunit/phpunit": "^6.5.14",
		"ockcyp/covers-validator": "~1.1",
		"squizlabs/php_codesniffer": "~3.3",
		"slevomat/coding-standard": "^3.0|~4.5",
		"mediawiki/mediawiki-codesniffer": "~23.0",
		"wikimedia/testing-access-wrapper": "^1.0"
	},
	"autoload": {
		"psr-4": {
			"Wikibase\\TermStore\\MediaWiki\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Wikibase\\TermStore\\MediaWiki\\Tests\\": "tests/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "0.1.x-dev"
		}
	}
}
